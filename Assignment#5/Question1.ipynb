   "source": [
    "#Question1\n",
    "def calFactorial(num):\n",
    "    if num == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return num * calFactorial(num-1)\n",
    "number = int(input(\"Enter number to calculate \"))\n",
    "answer = calFactorial(number)\n",
    "print(\"The factorial of number \"+str(number)+\" is \"+str(answer))\n"
   ]
