    "#Question3\n",
    "def checkEven(List):\n",
    "    EvenList = []\n",
    "    for i in List:\n",
    "        if i%2==0:\n",
    "            EvenList.append(i)\n",
    "    print(\"Original List==>\",List)\n",
    "    print(\"List of Even Numbers From Given List==>\",EvenList)\n",
    "    \n",
    "Alllist = [2,4,5,9,10,6,8,11,13,1,43,22,44]\n",
    "checkEven(Alllist)"
